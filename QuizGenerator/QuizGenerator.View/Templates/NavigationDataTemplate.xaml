<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vms="clr-namespace:QuizGenerator.ViewModel.ViewModels;assembly=QuizGenerator.ViewModel"
                    xmlns:pages="clr-namespace:QuizGenerator.View.Views.Pages">
    <DataTemplate DataType="{x:Type vms:StartViewModel}">
        <pages:StartPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:SelectViewModel}">
        <pages:SelectPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:QuizPageViewModel}">
        <pages:QuizPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:QuestionPageViewModel}">
        <pages:QuestionPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:TrainingViewModel}">
        <pages:TrainingPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vms:QuestionDetailPageViewModel}">
        <pages:QuestionDetailPage/>
    </DataTemplate>
</ResourceDictionary>
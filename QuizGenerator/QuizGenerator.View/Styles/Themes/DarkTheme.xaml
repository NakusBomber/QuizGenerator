<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">#16191c</Color>
    <Color x:Key="BackgroundSecondaryColor">#23262b</Color>

    <Color x:Key="BackdropColor">MintCream</Color>

    <Color x:Key="PrimaryColor">#3d55d1</Color>
    <Color x:Key="HoverPrimaryColor">#1c2763</Color>
    <Color x:Key="PressedPrimaryColor">#0f163d</Color>
    <Color x:Key="DisablePrimaryColor">#3e3e3e</Color>

    <Color x:Key="SecondaryColor">#6B7280</Color>
    <Color x:Key="HoverSecondaryColor">#3f4654</Color>
    <Color x:Key="PressedSecondaryColor">#0f163d</Color>
    <Color x:Key="DisableSecondaryColor">#3e3e3e</Color>

    <Color x:Key="TextColor">#e1e2e2</Color>
    <Color x:Key="DisableTextColor">#949595</Color>
</ResourceDictionary>